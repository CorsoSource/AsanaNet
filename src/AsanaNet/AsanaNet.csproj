<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <GenerateAssemblyInfo>false</GenerateAssemblyInfo><!-- setting GenerateAssemblyInfo to false causes this bug https://github.com/dotnet/project-system/issues/3934 -->
    <TargetFramework>net8.0</TargetFramework>
    <AssemblyName>AsanaNet</AssemblyName>
    <PackageId>CorsoSystems.AsanaNet</PackageId>
    <OutputType>Library</OutputType>
    <Authors>William Bohn</Authors>
    <Company>Corso Systems</Company>
    <AssemblyTitle>OpenAPI Library</AssemblyTitle>
    <Description>An Asana client generated from the OpenAPI specification</Description>
    <Copyright>No Copyright</Copyright>
    <RootNamespace>AsanaNet</RootNamespace>
    <Version>1.0.0</Version>
    <DocumentationFile>bin\$(Configuration)\$(TargetFramework)\AsanaNet.xml</DocumentationFile>
    <RepositoryUrl>https://github.com/CorsoSource/AsanaNet</RepositoryUrl>
    <RepositoryType>git</RepositoryType>
    <PackageReleaseNotes>Minor update</PackageReleaseNotes>
    <Nullable>annotations</Nullable>
    <GeneratePackageOnBuild>True</GeneratePackageOnBuild>
    <Title>AsanaNet</Title>
    <PackageProjectUrl>https://github.com/CorsoSource/AsanaNet</PackageProjectUrl>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="JsonSubTypes" Version="2.0.1" />
    <PackageReference Include="Newtonsoft.Json" Version="13.0.3" />
    <PackageReference Include="RestSharp" Version="110.2.0" />
    <PackageReference Include="Polly" Version="8.1.0" />
  </ItemGroup>

  <ItemGroup>
    <None Remove="System.Web" />
  </ItemGroup>
  <ItemGroup>
    <Reference Include="System.Web" />
  </ItemGroup>
</Project>
